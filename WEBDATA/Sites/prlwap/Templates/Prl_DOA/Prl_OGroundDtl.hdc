;Sql2=select a.farea fareaSum, nvl(b.farea, 0) yAreaSum, a.farea - nvl(b.farea, 0) nAreaSum from (select g.floorno, nvl(sum(g.farea), 0) farea from v_PRL_Ground_Floor g where g.fno like '^^sel_Type^^%' and g.floorno = '^^txt_floorno^^' group by g.floorno) a, (select g.floorno,  nvl(sum(g.farea), 0) farea,  nvl(sum(g.tarea), 0) tarea from v_Prl_OIsf_Floor g where g.fno like '^^sel_Type^^%' and g.floorno = '^^txt_floorno^^' group by g.floorno) b where a.floorno = b.floorno(+)

;
SqlForCondition3=select sum(f.farea) fAreaSum from v_PRL_Ground_Floor f where f.fno like '^^sel_Type^^%' and f.floorno = '^^txt_floorno^^'

