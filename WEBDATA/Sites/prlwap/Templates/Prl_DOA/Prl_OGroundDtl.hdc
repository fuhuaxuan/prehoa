[Master]
;显示，多条记录的查询，可在HDT页面中直接使用<HDLoop>显示，并能够提供翻页。and '^^NotFirst^^' = '1'
SQL=select * from (select f.fno,0 yarea,f.farea narea from v_PRL_Ground_Floor f where f.fno like '^^sel_Type^^%' and f.floorno = '^^txt_floorno^^' and not exists (select 1 from v_Prl_OIsf_Floor t where t.fno = f.fno and t.floorno = f.floorno ) union all select ISFlessee fno,tarea yarea,0 narea from (select nvl(sum(t.tarea),0) tarea,t.isfnum,t.ISFlessee from v_Prl_OIsf_Floor t where t.fno like '^^sel_Type^^%' and t.floorno = '^^txt_floorno^^' group by t.isfnum,t.ISFlessee) f) o where 1=1

Count=select count(*) from (select f.fno,0 yarea,f.farea narea from v_PRL_Ground_Floor f where f.fno like '^^sel_Type^^%' and f.floorno = '^^txt_floorno^^' and not exists (select 1 from v_Prl_OIsf_Floor t where t.fno = f.fno and t.floorno = f.floorno ) union all select ISFlessee fno,tarea yarea,0 narea from (select nvl(sum(t.tarea),0) tarea,t.isfnum,t.ISFlessee from v_Prl_OIsf_Floor t where t.fno like '^^sel_Type^^%' and t.floorno = '^^txt_floorno^^' group by t.isfnum,t.ISFlessee) f) o where 1=1


