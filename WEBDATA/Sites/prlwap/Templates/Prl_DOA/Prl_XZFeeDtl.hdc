[Master]
;显示，多条记录的查询，可在HDT页面中直接使用<HDLoop>显示，并能够提供翻页。and '^^NotFirst^^' = '1'
SQL=select t.*,decode(t.typye,1,'','none') disp,decode(t.typye,1,'1','5') cols,decode(t.typye,1,'left','right') salign,decode(t.typye,1,t.FILLDEPTNAME,2,t.payman || ' MTD expenses (员工月份总额)',t.FILLDEPTNAME||' Mthly expenses (部门月份总额)') DeptName,decode(t.typye,1,'',2,'#FFFFCC','#42c3ea') sColor,decode(t.typye,1,t.PayMan,'') PayManStr,decode(t.typye,1,t.Position,'') PositionStr,decode(t.typye,1,t.feedate,'') feedateStr,decode(t.typye,1,t.feeedate,'') feeedateStr,to_char(t.f1Fee,'FM999,999,999,990.90') f1FeeStr,to_char(t.f2Fee,'FM999,999,999,990.90') f2FeeStr,to_char(t.f3Fee,'FM999,999,999,990.90') f3FeeStr,to_char(t.f4Fee,'FM999,999,999,990.90') f4FeeStr,to_char(t.f5Fee,'FM999,999,999,990.90') f5FeeStr,to_char(t.fSumFee,'FM999,999,999,990.90') fSumFeeStr from (select 1 typye, t.feey, t.feem, t.COMGID, t.COMNAME, t.FILLDEPTNAME, t.payman, t.position, to_char(t.feedate,'DD/MM/YYYY') feedate,to_char(t.feeedate,'DD/MM/YYYY') feeedate, t.f1fee, t.f2fee, t.f3fee, t.f4fee, t.f5fee, t.FSumFee,t.memo,t.Modelname from v_prl_baoxiao_fee t union select 2 typye, t.feey, t.feem, t.COMGID, t.COMNAME, t.FILLDEPTNAME, t.payman, '' position, '' feedate, '' feeedate, sum(t.f1fee) f1fee, sum(t.f2fee) f2fee, sum(t.f3fee) f3fee, sum(t.f4fee) f4fee, sum(t.f5fee) f5fee, sum(t.FSumFee) FSumFee,'' memo,'' Modelname from v_prl_baoxiao_fee t group by t.feey,t.feem,t.COMGID,t.COMNAME,t.FILLDEPTNAME,t.payman union select 3 typye, t.feey, t.feem, t.COMGID, t.COMNAME, t.FILLDEPTNAME, '' payman, '' position, '' feedate, '' feeedate, sum(t.f1fee) f1fee, sum(t.f2fee) f2fee, sum(t.f3fee) f3fee, sum(t.f4fee) f4fee, sum(t.f5fee) f5fee, sum(t.FSumFee) FSumFee,'' memo,'' Modelname from v_prl_baoxiao_fee t group by t.feey, t.feem, t.COMGID, t.COMNAME, t.FILLDEPTNAME) t where '^^NotFirst^^' = '1'

Count=select Count(*) from (select 1 typye, t.feey, t.feem, t.COMGID, t.COMNAME, t.FILLDEPTNAME, t.payman, t.position, to_char(t.feedate,'DD/MM/YYYY') feedate,to_char(t.feeedate,'DD/MM/YYYY') feeedate, t.f1fee, t.f2fee, t.f3fee, t.f4fee, t.f5fee, t.FSumFee,t.memo,t.Modelname from v_prl_baoxiao_fee t union select 2 typye, t.feey, t.feem, t.COMGID, t.COMNAME, t.FILLDEPTNAME, t.payman, '' position, '' feedate, '' feeedate, sum(t.f1fee) f1fee, sum(t.f2fee) f2fee, sum(t.f3fee) f3fee, sum(t.f4fee) f4fee, sum(t.f5fee) f5fee, sum(t.FSumFee) FSumFee,'' memo,'' Modelname from v_prl_baoxiao_fee t group by t.feey,t.feem,t.COMGID,t.COMNAME,t.FILLDEPTNAME,t.payman union select 3 typye, t.feey, t.feem, t.COMGID, t.COMNAME, t.FILLDEPTNAME, '' payman, '' position, '' feedate, '' feeedate, sum(t.f1fee) f1fee, sum(t.f2fee) f2fee, sum(t.f3fee) f3fee, sum(t.f4fee) f4fee, sum(t.f5fee) f5fee, sum(t.FSumFee) FSumFee,'' memo,'' Modelname from v_prl_baoxiao_fee t group by t.feey, t.feem, t.COMGID, t.COMNAME, t.FILLDEPTNAME) t where '^^NotFirst^^' = '1'

