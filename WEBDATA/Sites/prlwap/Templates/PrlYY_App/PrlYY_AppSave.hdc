[AddtionalSQL]
;操作，没有返回的结果，是在数据库中执行，有Insert, Update, Delete命令。从SQL1～SQL20共支持20条

;删除
SQL1=delete from PrlYY_App where EntGid = __ENTGID__ and ComGid = ##mComGid##

;插入明细表
LoopSql1=insert into PrlYY_App(EntGid,Gid,ComGid,ComCode,ComName,PostGid,PostCode,PostName,AppGid,AppCode,AppName,ModelGid,ModelCode,ModelName) select __ENTGID__,sys_guid(), ##%ComGid%##,##%ComCode%##,##%ComName%##,##%PostGid%##,##%PostCode%##,##%PostName%##,##%BAppGid%##,##%BAppCode%##,##%BAppName%##,m.ModelGid,m.Code,m.Name from wf_model m where m.EntGid = __ENTGID__ and lower(m.code) = 'PrlYY_baoxiao'

LoopSql2=insert into PrlYY_App(EntGid,Gid,ComGid,ComCode,ComName,PostGid,PostCode,PostName,AppGid,AppCode,AppName,ModelGid,ModelCode,ModelName) select __ENTGID__,sys_guid(), ##%ComGid%##,##%ComCode%##,##%ComName%##,##%PostGid%##,##%PostCode%##,##%PostName%##,##%FAppGid%##,##%FAppCode%##,##%FAppName%##,m.ModelGid,m.Code,m.Name from wf_model m where m.EntGid = __ENTGID__ and lower(m.code) = 'PrlYY_fee'

LoopSql3=insert into PrlYY_App(EntGid,Gid,ComGid,ComCode,ComName,PostGid,PostCode,PostName,AppGid,AppCode,AppName,ModelGid,ModelCode,ModelName) select __ENTGID__,sys_guid(), ##%ComGid%##,##%ComCode%##,##%ComName%##,##%PostGid%##,##%PostCode%##,##%PostName%##,##%PAppGid%##,##%PAppCode%##,##%PAppName%##,m.ModelGid,m.Code,m.Name from wf_model m where m.EntGid = __ENTGID__ and lower(m.code) = 'PrlYY_pay'

