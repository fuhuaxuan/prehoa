[Permission]
;判断FlowGid 是否为空
LIMIT1=FlowGid
LIMITDesc1=StringSize[,0]
LIMITRemind1=对不起，您未指定流程信息！请从正确的路径进入

;判断传入FlowGid 是否存在
SQL1=select 1 from WF_Flow where EntGid = __ENTGID__ and FlowGid = ##FlowGid##
Remind1=对不起，您指定的流程申请单没有找到<script>alert('对不起，您指定的流程申请单没有找到');window.close()</script>

[Detail]
;显示，单条数据的查询，可以用于HDT显示，有select命令。从SQL1～SQL20共支持20条
;取出本流程模型信息
SqlForCondition1=select wm.code ModelCode, wm.Name ModelName,d.Name StepName from wf_Model wm,WF_Task_Define d where wm.EntGid = d.EntGid(+) and wm.EntGid = __ENTGID__ and wm.ModelGid = d.ModelGid(+) and wm.ModelGid = ##ModelGid## and lower(d.code(+)) = replace(lower(##Url##),'save')

;取出当前用户的类别及排序
SqlForCondition2=select distinct AppOrder,AppType from Wf_^^ModelCode^^_App a where a.EntGid = __ENTGID__ and a.FlowGid = ##FlowGid## and AppGid = __USRGID__ and appdate is null

;取出本流程主表信息
SqlForCondition3=select substr(f.FillDeptCode, 0, 4) ParentDeptCode,f.* from wf_^^ModelCode^^ f where f.EntGid = __ENTGID__ and f.FlowGid = ##FlowGid##

SQL2=select decode(count(*),0,'none','') isShow from Wf_^^ModelCode^^_App where EntGid = __ENTGID__ and FlowGid = ##FlowGid## and AppOrder > 0 and AppOrder < ##AppOrder## and appdate is not null

SQL3=select AppGid LastGid,AppCode LastCode,AppName LastName,AppType LastType,AppOrder LastOrder from Wf_^^ModelCode^^_App a where a.EntGid = __ENTGID__ and a.FlowGid = ##FlowGid## and a.AppDate is not null and a.AppOrder in (select max(t.AppOrder) from Wf_^^ModelCode^^_App t where t.EntGid = __ENTGID__ and t.FlowGid = ##FlowGid## and t.AppOrder < ##AppOrder## and t.appdate is not null)

SQL4=select AppGid NextGid from Wf_^^ModelCode^^_App a where a.EntGid = __ENTGID__ and a.FlowGid = ##FlowGid## and a.AppDate is null and a.AppOrder in (select min(t.AppOrder) from Wf_^^ModelCode^^_App t where t.EntGid = __ENTGID__ and t.FlowGid = ##FlowGid## and t.AppOrder > ##AppOrder## and t.appdate is null)

;得到下一批审批人信息
LoopSQL1=select AppGid ItemGid, AppName ItemName from Wf_^^ModelCode^^_App a where a.EntGid = __ENTGID__ and a.FlowGid = ##FlowGid## and a.AppDate is null and a.AppOrder in (select min(t.AppOrder) from Wf_^^ModelCode^^_App t where t.EntGid = __ENTGID__ and t.FlowGid = ##FlowGid## and t.AppOrder > ##AppOrder## and t.appdate is null)
LoopHDT1=Templates\WorkFlow\Form\^^ModelCode^^\LoopSql_Option.hdt
LoopVAR1=AppList

;取出抄送人
;1.项目财务经理 30
;2.区域总经理 80
;3.集团财务总监 & 副总监
;4.中国区CEO
LoopSQL2=select distinct v.PostGid ItemGid from v_Post v where v.EntGid = __ENTGID__ and v.deptGid = ##FillDeptGid## and v.atype in (25,30,80) union select distinct o.AppGid ItemGid from v_wf_model_usr_app o where o.EntGid = __ENTGID__ and o.ModelGid = ##ModelGid## and replace(lower(o.Modelcode), lower(##ModelCode##), '') in ('_tc0','_tc1','_td1')
LoopHDT2=Templates\WorkFlow\Form\^^ModelCode^^\LoopSql_Option.hdt
LoopVAR2=TccList
