[Permission]
;判断FlowGid 是否为空
LIMIT1=FlowGid
LIMITDesc1=StringSize[,0]
LIMITRemind1=对不起，您未指定流程信息！请从正确的路径进入

;判断传入FlowGid 是否存在
SQL1=select 1 from wf_Flow where EntGid = __ENTGID__ and FlowGid = ##FlowGid##
Remind1=对不起，您指定的流程申请单没有找到<script>alert('对不起，您指定的流程申请单没有找到');window.close()</script>

[Detail]
;显示，单条数据的查询，可以用于HDT显示，有select命令。从SQL1～SQL20共支持20条
;取出本流程模型信息
SqlForCondition1=select wm.Code ModelCode, wm.Name ModelName from wf_Model wm where wm.EntGid = __ENTGID__ and wm.ModelGid = ##ModelGid##


;取出旧的ISF/ISCF的信息
SQL1=select f.ModelGid ISFModelGid,m.code ISFModelCode,f.FlowGid ISFFlowGid,f.Num ISFNum,f.Atype,f.Lessee,f.Tradingname,f.Contactor,f.Address,f.Phone,f.Email,f.Fax,f.LeaseTermY,f.LeaseTermM,f.LeaseTermD,f.HandoverDate,f.FreeRentM,f.FreeRentD,f.ContractDate1,f.Contractdate2,f.Rate,f.AGR,f.Security,f.Incash,f.BRCNew,f.BRCExist,f.BRCBudget,f.TotalFee,f.Memo from WF_PrlGG_ISF f,wf_model m,WF_^^ModelCode^^ o where f.FlowGid = o.OldFlowGid and f.EntGid=__ENTGID__ and f.EntGid = m.EntGid and f.modelgid = m.modelgid and o.FlowGid = ##FlowGid##

;得到业务单据信息
Sql2=select wf.*,'广告位ISTF' MCode from WF_^^ModelCode^^ wf where EntGid = __ENTGID__ and FlowGid = ##FlowGid##

;取出当前流程，当前用户，下一步操作
SQl3=select wt.Code NextStepCode,wt.Name NextStepName from wf_task wt where wt.EntGid = __ENTGID__ and wt.ModelGid = ##ModelGid## and wt.FlowGid = ##FlowGid## and wt.ExecGid = __USRGID__ and wt.stat = 1

;财务经理及财务会计可以打印
SQL4=select decode(count(*),0,'none','') IsPrint from wf_prl_oistf wf,v_Post v where wf.EntGid = __ENTGID__ and wf.FlowGid = ##FlowGid## and wf.EntGid = v.EntGid and wf.FillDeptGid = v.deptGid and v.atype in (25,30) and v.PostGid = __USRGID__

SQL5=select count(*) IsAdmin from v_usr u, v_org_usr v where u.entgid = __ENTGID__ and u.entgid = v.entgid and u.GID = v.ORGGID and v.USRGID = __USRGID__ and lower(u.code) = 'admin_grp'

LoopSQL3=select fd.TBID,to_char(fd.FreeRentDate1,'YYYY.MM.DD') FreeRentDate1,to_char(fd.FreeRentDate2,'YYYY.MM.DD') FreeRentDate2,fd.yeartype,to_char(fd.TBRD,'FM999999999990.9990') tbrd,to_char(fd.tbrm,'FM999999999990.9990') tbrm from wf_PrlGG_ISF_dtl fd where fd.EntGid = __ENTGID__ and fd.FlowGid = ##FlowGid## and lower(fd.TBID) in ('tb_dtl10','tb_dtl30') order by fd.TBID,fd.yeartype
LoopHDT3=Templates\WorkFlow\Form\PrlGG_ISF\LoopSql_EmailDtl.hdt
LoopVAR3=EmailDtl

;显示单位信息
LoopSQL6=select * from wf_PrlGG_ISF_ground where EntGid = __ENTGID__ and FlowGid = ##FlowGid## order by fNo
LoopHDT6=Templates\WorkFlow\Form\PrlGG_ISF\LoopSql_UnitList.hdt
LoopVAR6=UnitList

;显示邮件
LoopSQL19=select 1 from dual
LoopHDT19=Templates\WorkFlow\Form\^^ModelCode^^\Loopsql_Email.hdt
LoopVAR19=EmailList

LoopSQL20=select 1 from dual
LoopHDT20=Templates\WorkFlow\Form\PrlDZ_Email\Loopsql_Email.hdt
LoopVAR20=EmailPhone
