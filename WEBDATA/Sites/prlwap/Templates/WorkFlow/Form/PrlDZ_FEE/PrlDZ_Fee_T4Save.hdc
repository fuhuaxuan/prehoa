[Permission]
;判断FlowGid 是否为空
LIMIT1=FlowGid
LIMITDesc1=StringSize[,0]
LIMITRemind1=对不起，您未指定流程信息！请从正确的路径进入

;判断传入FlowGid 是否存在
SQL1=select 1 from WF_Flow where EntGid = __ENTGID__ and FlowGid = ##FlowGid##
Remind1=对不起，您指定的流程申请单没有找到<script>alert('对不起，您指定的流程申请单没有找到');window.close()</script>

[Detail]
;显示，单条数据的查询，可以用于HDT显示，有select命令。从SQL1～SQL20共支持20条
;取出本流程模型信息
SqlForCondition1=select wm.code ModelCode,replace(upper(wm.code),'DZ','') ModelCodeT, wm.Name ModelName,d.Name StepName from wf_Model wm,WF_Task_Define d where wm.EntGid = d.EntGid(+) and wm.EntGid = __ENTGID__ and wm.ModelGid = d.ModelGid(+) and wm.ModelGid = ##ModelGid## and lower(d.code(+)) = replace(lower(##Url##),'save')

SQLForCondition2=select ##CurUsrName##||':'||##AppAssign##||' - 申请金额：' || Askfee || '；申请目的：' || Target AppFlowMemo from wf_^^ModelCodeT^^ where EntGid = __ENTGID__ AND  FlowGid = ##FlowGid## 

;取出抄送人
SqlForCondition3=select v.PostGid TccGid from v_Post v where v.EntGid = __ENTGID__ and v.deptGid = ##FillUsrDeptGid## and v.atype=30

[AddtionalSQL]
;操作，没有返回的结果，是在数据库中执行，有Insert, Update, Delete命令。从SQL1～SQL20共支持20条
;更新本流程中主表信息
Sql1=update wf_^^ModelCodeT^^ set (LastUpdDate,Stat,confirmdate,confirmfee,Nconfirmfee) = (select sysdate,'已完成',sysdate,##confirmfee##,##Nconfirmfee## from dual) where EntGid = __ENTGID__ AND  FlowGid = ##FlowGid## 

;插入本流程各级审批人信息
Sql2=insert into wf_^^ModelCodeT^^_App (EntGid,FlowGid,dtlgid,AppGid,AppCode,AppName,AppType,AppAssign,note,AppDate) select __ENTGID__,##FlowGid##,sys_guid(),__USRGID__,__USRCODE__,__USRNAME__, 120,##AppAssign##,##AppMemo##,sysdate from dual

;更新项目的预算费用
SQL4=update PRL_ACG_COMPANY o set (LeftBudgutFee,OpUsrGid,OpUsrCode,OpUsrName,OpDate,OpMemo,OpModelCode,OpFlowGid,OpNum) = (select o.LeftBudgutFee - nvl(##confirmfee##,0),__USRGID__,__USRCODE__,__USRNAME__,sysdate,'费用申请单扣减',##ModelCode##,##FlowGid##,##Num## from dual) where o.entgid = __ENTGID__ and exists (select 1 from WF_^^ModelCodeT^^ wf where wf.entgid = __ENTGID__ and wf.flowgid = ##FlowGid## and o.ACGGID = wf.acgtwogid and o.CompanyGid = wf.CompanyGid and to_char(wf.CreateDate,'YYYY') = o.Year) 

SQL5=update PRL_ACG_COMPANY o set (LeftBudgutFee,OpUsrGid,OpUsrCode,OpUsrName,OpDate,OpMemo,OpModelCode,OpFlowGid,OpNum) = (select o.LeftBudgutFee - nvl(##Nconfirmfee##,0),__USRGID__,__USRCODE__,__USRNAME__,sysdate,'费用申请单扣减',##ModelCode##,##FlowGid##,##Num## from dual) where o.entgid = __ENTGID__ and exists (select 1 from WF_^^ModelCodeT^^ wf where wf.entgid = __ENTGID__ and wf.flowgid = ##FlowGid## and o.ACGGID = wf.acgtwogid and o.CompanyGid = wf.CompanyGid and to_char(add_months(wf.CreateDate,12),'YYYY') = o.Year) 

SQL6=insert into PRL_ACG_COMPANY (EntGid,Gid,CompanyGid,AcgGid,LeftBudgutFee,Year,OpUsrGid,OpUsrCode,OpUsrName,OpDate,OpMemo,OpModelCode,OpFlowGid,OpNum) select __ENTGID__,sys_guid(),wf.CompanyGid,wf.acgtwogid,0-nvl(##Nconfirmfee##,0),to_char(add_months(wf.CreateDate,12),'YYYY'),__USRGID__,__USRCODE__,__USRNAME__,sysdate,'费用申请单扣减',##ModelCode##,##FlowGid##,##Num## from WF_^^ModelCodeT^^ wf where wf.entgid = __ENTGID__ and wf.flowgid = ##FlowGid## and not exists (select 1 from PRL_ACG_COMPANY o where wf.entgid = __ENTGID__ and wf.flowgid = ##FlowGid## and o.ACGGID = wf.acgtwogid and o.CompanyGid = wf.CompanyGid and to_char(add_months(wf.CreateDate,12),'YYYY') = o.Year) 

;插入本流程中审批人二信息
SQL11=update Wf_^^ModelCodeT^^_App set (AppAssign,note,AppDate) = (select '同意', '',sysdate from dual) where EntGid = __ENTGID__ and FlowGid = ##FlowGid## and AppOrder = 100

;删除本流程全部附件信息
SQL12=delete from WF_Prl_Flow_Attach where EntGid = __ENTGID__ and FlowGid = ##FlowGid##

AttachSqlAdd1=insert into WF_Prl_Flow_Attach (EntGid,ModelGid,FlowGid,Attach_Gid,Attach_Title,Attach_Url,Attach_Size) select __ENTGID__,##ModelGid## ,##FlowGid##,@@AttachID@@, @@FileName@@, @@FilePath@@,@@FileSize@@ from dual
