;版本号：2014年3月份（此行不允许删除）
[Permission]
;判断FlowGid 是否为空
LIMIT1=FlowGid
LIMITDesc1=StringSize[,0]
LIMITRemind1=对不起，您未指定流程信息！请从正确的路径进入

;判断传入FlowGid 是否存在
SQL1=select 1 from wf_Flow where EntGid = __ENTGID__ and FlowGid = ##FlowGid##
Remind1=对不起，您指定的流程申请单没有找到<script>alert('对不起，您指定的流程申请单没有找到');window.close()</script>

[Detail]
;显示，单条数据的查询，可以用于HDT显示，有select命令。从SQL1～SQL20共支持20条
;取出本流程模型信息
SqlForCondition1=select wm.Code ModelCode, wm.Name ModelName,d.Name StepName from wf_Model wm,WF_Task_Define d where wm.EntGid = d.EntGid(+) and wm.EntGid = __ENTGID__ and wm.ModelGid = d.ModelGid(+) and wm.ModelGid = ##ModelGid## and lower(d.code(+)) = replace(lower(##Url##),'save')

;取出发起人的部门代码
SqlForCondition2=select substr(f.FillUsrDeptCode,0,4) ParentDeptCode from Wf_PRL_ISTF f where f.EntGid = __ENTGID__ and f.FlowGid = ##FlowGid##

;取出旧的ISF/ISCF的信息
SqlForCondition3=select f.num OldNum,f.modelgid OldModelGid,m.code OldModelCode,f.FlowGid OldFlowGid,f.DEPT,f.area,f.BuildArea,f.tradename,f.trade,f.lessee,f.tradingname,f.address,f.contactor,f.phone,f.email,f.fax,f.leaseTermY,f.leaseTermM,f.leaseTermD,to_char(f.HandoverDate,'YYYY.MM.DD') HandoverDate,f.Related,f.Detail,f.Rate1,f.Rate,to_char(f.contractDate1,'YYYY.MM.DD') contractDate1,to_char(f.contractdate2,'YYYY.MM.DD') contractdate2,f.freeRentM,f.freeRentD,f.fitoutM,f.fitoutD,to_char(f.fitoutdate1,'YYYY.MM.DD') fitoutdate1,to_char(f.fitoutdate2,'YYYY.MM.DD') fitoutdate2,f.collection,Security from WF_PRL_ISF f,wf_model m,Wf_PRL_ISTF o where f.FlowGid = o.OldFlowGid and f.EntGid=__ENTGID__ and f.EntGid = m.EntGid and f.modelgid = m.modelgid and o.FlowGid = ##FlowGid##

SqlForCondition4=select max(yeartype) maxyeartype from wf_PRL_ISF_dtl a where EntGid=__ENTGID__ and FlowGid = ##OldFlowGid##  and lower(a.TBID) in ('tb_dtl30')

Sql2=select * from Wf_PRL_ISTF where EntGid = __ENTGID__ and  FlowGid = ##FlowGid##

;取出本流程已审批的审批人的个数，用于判断是否显示审批人
SQL3=select count(*) IsShow from Wf_Prl_Isf_App where EntGid = __ENTGID__ and  FlowGid = ##FlowGid##

;显示本流程附件信息
LoopSQL1=select Attach_Title ItemGid,Attach_Title ItemName from WF_PRL_ISF_Attach where EntGid = __ENTGID__ and  FlowGid = ##FlowGid## order by attach_title
LoopHDT1=Templates\WorkFlow\Form\^^ModelCode^^\LoopSql_Option.hdt
LoopVAR1=AttachList

;显示本流程审批人信息
LoopSQL2=select a.*,decode(a.apptype,0,'发起人',10,'部门经理',32,'部门总经理',30,'财务经理',35,'总经理',40,'项目总经理',50,'资产经理',60,'租赁部总监',70,'副财务总监',80,'财务总监',85,'资产经理',90,'区域总经理',95,'中国区CEO',100,'集团CEO',110,'发起人确认') apptypetext from Wf_Prl_Isf_App a where FlowGid = ##FlowGid## order by AppDate,AppType
LoopHDT2=Templates\WorkFlow\Form\^^ModelCode^^\Loopsql_AppList.hdt
LoopVAR2=AppList

;显示本流程明细信息
LoopSQL3=select * from Wf_PRL_ISTF_Dtl where EntGid = __ENTGID__ and FlowGid = ##FlowGid## order by Line
LoopHDT3=Templates\WorkFlow\Form\^^ModelCode^^\Loopsql_DtlMod.hdt
LoopVAR3=DtlList

;显示单位信息
LoopSQL4=select g.* from wf_PRL_ISF_ground g,Wf_PRL_ISTF f where f.EntGid = __ENTGID__ and f.FlowGid = ##FlowGid## and g.FlowGid = f.OldFlowGid order by g.fNo
LoopHDT4=Templates\WorkFlow\Form\^^ModelCode^^\LoopSql_UnitDtl.hdt
LoopVAR4=UnitList

LoopSQL5=select d.TBRD,d.PMFD,d.Apfixed,decode(lower(d.TBID),'tb_dtl10',to_char(freerentdate1,'YYYY.MM.DD'),decode(yeartype,1,to_char(add_months(to_date('^^contractDate1^^','YYYY.MM.DD'),nvl('^^freeRentM^^',0))+nvl('^^freeRentD^^',0),'YYYY.MM.DD'),to_char(add_months(to_date('^^contractDate1^^','YYYY.MM.DD'), ((yeartype) - 1) * 12), 'YYYY.MM.DD'))) sDate,decode(lower(d.TBID),'tb_dtl10',to_char(freerentdate2,'YYYY.MM.DD'),decode(yeartype,'^^maxyeartype^^','^^contractdate2^^',to_char(add_months(to_date('^^contractDate1^^','YYYY.MM.DD'), (yeartype) * 12) - 1, 'YYYY.MM.DD'))) eDate from wf_PRL_ISF_Dtl d where EntGid = __ENTGID__ and FlowGid = ##OldFlowGid## and lower(d.TBID) in ('tb_dtl10','tb_dtl30') order by TbID,YearType,FreeRentDate1
LoopHDT5=Templates\WorkFlow\Form\PRLDZ_ISTF\Loopsql_ISFDtl.hdt
LoopVAR5=ISFDtlList

;显示TA1的审批人
;鹏瑞利管辖部门的扩展性：5:部门助理;10:部门经理(招商经理);20:人事经理;30:财务经理;31:出纳;40:项目总经理;50:区域总监;71:资产经理 AssetManager
LoopSQL6=select distinct v.PostGid ItemGid,v.PostName ItemName from v_Post v,Wf_PRL_ISTF f where f.EntGid = __ENTGID__ and f.EntGid = v.EntGid and f.FlowGid = ##FlowGid## and v.deptGid = f.FillUsrDeptGid and v.atype=10 order by v.PostName
LoopHDT6=Templates\WorkFlow\Form\^^ModelCode^^\LoopSql_Option.hdt
LoopVAR6=T2List

;显示本流程明细信息
LoopSQL8=select * from Wf_PRL_ISTF_Fee where EntGid = __ENTGID__ and FlowGid = ##FlowGid## order by Line
LoopHDT8=Templates\WorkFlow\Form\^^ModelCode^^\Loopsql_FeeMod.hdt
LoopVAR8=FeeList

