[Permission]
;判断FlowGid 是否为空
LIMIT1=FlowGid
LIMITDesc1=StringSize[,0]
LIMITRemind1=对不起，您未指定流程信息！请从正确的路径进入

;判断传入FlowGid 是否存在
SQL1=select 1 from WF_Flow wf where wf.EntGid = __ENTGID__ and wf.FlowGid = ##FlowGid##
Remind1=对不起，您指定的流程申请单没有找到<script>alert('对不起，您指定的流程申请单没有找到');window.close()</script>

[Detail]
;显示，单条数据的查询，可以用于HDT显示，有select命令。从SQL1～SQL20共支持20条
;取出本流程模型信息
SqlForCondition1=select wm.Code ModelCode, wm.Name ModelName,'广告位'||replace(upper(wm.code),'PRLGG_','') MCode from wf_Model wm where wm.EntGid = __ENTGID__ and wm.ModelGid = ##ModelGid##

SQL10=select platform,registrationid from usr where EntGid = __ENTGID__ and Gid in (select ExecGid from (select t.* from wf_task t where t.EntGid = __ENTGID__ and t.FlowGid = ##FlowGid## and t.Stat = 1 order by t.createdate desc) where rownum = 1 )

;显示本流程主表信息
SQL1=select substr(wf.FillDeptCode, 0, 4) ParentDeptCode,wf.* from wf_PrlGG_ISF wf where wf.EntGid = __ENTGID__ and wf.FlowGid = ##FlowGid##

;显示本流程附件信息
LoopSQL1=select Attach_Title,replace(Attach_Url,' ','%20') Attach_Url from wf_PrlGG_ISF_Attach where EntGid = __ENTGID__ and  FlowGid = ##FlowGid## order by attach_title
LoopHDT1=Templates\WorkFlow\Form\PrlGG_ISF\Loopsql_AttachList.hdt
LoopVAR1=AttachList

LoopSQL3=select fd.TBID,to_char(fd.FreeRentDate1,'YYYY.MM.DD') FreeRentDate1,to_char(fd.FreeRentDate2,'YYYY.MM.DD') FreeRentDate2,fd.yeartype,to_char(fd.TBRD,'FM999999999990.9990') tbrd,to_char(fd.tbrm,'FM999999999990.9990') tbrm from wf_PrlGG_ISF_dtl fd where fd.EntGid = __ENTGID__ and fd.FlowGid = ##FlowGid## and lower(fd.TBID) in ('tb_dtl10','tb_dtl30') order by fd.TBID,fd.yeartype
LoopHDT3=Templates\WorkFlow\Form\PrlGG_ISF\LoopSql_EmailDtl.hdt
LoopVAR3=EmailDtl

;显示单位信息
LoopSQL6=select * from wf_PrlGG_ISF_ground where EntGid = __ENTGID__ and FlowGid = ##FlowGid## order by fNo
LoopHDT6=Templates\WorkFlow\Form\PrlGG_ISF\LoopSql_UnitList.hdt
LoopVAR6=UnitList

;显示审批人
LoopSQL18=select hr.Email from hr_emp hr where hr.entgid = __ENTGID__ and hr.email is not null and exists (select 1 from wf_task t where t.EntGid = hr.EntGid and t.FlowGid = ##FlowGid## and t.ExecGid = hr.UsrGid and t.Stat = 1)
LoopHDT18=Templates\WorkFlow\Form\PrlDZ_Email\Loopsql_App.hdt
LoopVAR18=AppList

;显示邮件
LoopSQL19=select 1 from dual
LoopHDT19=Templates\WorkFlow\Form\PrlGG_ISF\Loopsql_Email.hdt
LoopVAR19=EmailList

LoopSQL20=select 1 from dual
LoopHDT20=Templates\WorkFlow\Form\PrlDZ_Email\Loopsql_WebEmail.hdt
LoopVAR20=EmailPhone
