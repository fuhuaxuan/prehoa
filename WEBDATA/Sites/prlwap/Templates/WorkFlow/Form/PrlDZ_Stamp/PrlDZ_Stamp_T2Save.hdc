[Permission]
;判断FlowGid 是否为空
LIMIT1=FlowGid
LIMITDesc1=StringSize[,0]
LIMITRemind1=对不起，您未指定流程信息！请从正确的路径进入

;判断传入FlowGid 是否存在
SQL1=select 1 from WF_Flow where EntGid = __ENTGID__ and FlowGid = ##FlowGid##
Remind1=对不起，您指定的流程申请单没有找到<script>alert('对不起，您指定的流程申请单没有找到');window.close()</script>

[Detail]
;显示，单条数据的查询，可以用于HDT显示，有select命令。从SQL1～SQL20共支持20条
;取出本流程模型信息
SqlForCondition1=select wm.Code ModelCode, wm.Name ModelName,d.Name StepName from wf_Model wm,WF_Task_Define d where wm.EntGid = d.EntGid(+) and wm.EntGid = __ENTGID__ and wm.ModelGid = d.ModelGid(+) and wm.ModelGid = ##ModelGid## and lower(d.code(+)) = replace(lower(##Url##),'save')

;审批备注
SQLForCondition2=select ##CurUsrName##||':'||##AppAssign##||' - 申请类型：' || ApplyType || '；申请印章种类：' || StampType || '；' || decode(ApplyType,'非合同类文件用印','','；合作公司：'||ContactName) AppFlowMemo,ApplyType,substr(FillDeptCode, 0, 4) PreDeptCode from WF_^^ModelCode^^ where EntGid = __ENTGID__ AND  FlowGid = ##FlowGid## 

;是否存在下一级审批人? 0 结束，到下一步
SQLForCondition4=select count(*) NextApp from Wf_^^ModelCode^^_App a where a.EntGid = __ENTGID__ and a.FlowGid = ##FlowGid## and a.AppDate is null and a.AppOrder in (select min(t.AppOrder) from Wf_^^ModelCode^^_App t where t.EntGid = __ENTGID__ and t.FlowGid = ##FlowGid## and t.AppOrder > ##AppOrder## and t.appdate is null)

;取出项目的财务经理
SQLForCondition5=select v.PostGid TccGid30 from v_Post v where v.EntGid = __ENTGID__ and v.deptGid = ##ComGid## and v.atype = 30 and rownum = 1

;取出项目副总监
SQLForCondition6=select v.PostGid TccGid13 from v_Post v where v.EntGid = __ENTGID__ and v.deptGid = ##FillDeptGid## and v.atype = 13 and rownum = 1

;是否存在下一级审批人? 0 结束，到下一步
SQLForCondition7=select count(*) NextTcc from v_wf_task a where a.EntGid = __ENTGID__ and a.FlowGid = ##FlowGid## and a.STAT = 1 and a.EXECGID <> __USRGID__ and a.ISMCF = 1 and lower(a.code) not in (lower(##ModelCode##||'_tcc'),lower(##ModelCode##||'_tc'))

SQLForCondition8=select decode(##NextApp##||##NextTcc##,'00',decode(##AppType##,'98','终止','提交'),'终止') AppStat from dual
[AddtionalSQL]
;操作，没有返回的结果，是在数据库中执行，有Insert, Update, Delete命令。从SQL1～SQL20共支持20条
;更新本流程中主表信息
Sql1=update WF_^^ModelCode^^ set (LastUpdDate,Stat) = (select sysdate,decode(##AppAssign##,'同意','审批中',##AppAssign##) from dual) where EntGid = __ENTGID__ AND  FlowGid = ##FlowGid## 

Sql3=delete from Wf_^^ModelCode^^_App where EntGid = __ENTGID__ and FlowGid = ##FlowGid## and AppGid = __USRGID__ and AppOrder = ##AppOrder##

Sql4=delete from Wf_^^ModelCode^^_App where EntGid = __ENTGID__ and FlowGid = ##FlowGid## and AppDate is null and ##AppAssign## = '否决'

;插入本流程中审批人一信息
SQL5=insert into Wf_^^ModelCode^^_App (EntGid,ModelGid,FlowGid,Gid,AppGid,AppCode,AppName,AppOrder,AppType,AppAssign,AppMemo,AppDate) select __ENTGID__,##ModelGid##,##FlowGid##,sys_guid(),__USRGID__,__USRCODE__,__USRNAME__,##AppOrder##,##AppType##,##AppAssign##, decode(##AppType##,'98',decode(##AppCheck##,'我确认盖章文件与附件一致',##AppCheck##,##AppCheck##) || '-','') || ##AppMemo##,sysdate from dual

SQL6=insert into Wf_^^ModelCode^^_App (EntGid,ModelGid,FlowGid,Gid,AppGid,AppCode,AppName,AppType,AppOrder) select __ENTGID__,##ModelGid##,##FlowGid##,sys_guid(),##LastGid##,##LastCode##,##LastName##,##LastType##,##LastOrder## from dual where ##AppAssign## = '返回核实'

SQL7=insert into Wf_^^ModelCode^^_App (EntGid,ModelGid,FlowGid,Gid,AppGid,AppCode,AppName,AppType,AppOrder) select __ENTGID__,##ModelGid##,##FlowGid##,sys_guid(),__USRGID__,__USRCODE__,__USRNAME__,##AppType##,##AppOrder## from dual where ##AppAssign## = '返回核实'

SQL8=call P_^^ModelCode^^_doApp2(__ENTGID__,##ModelGid##,##FlowGid##,##AppStat##,##AppFlowMemo##)

