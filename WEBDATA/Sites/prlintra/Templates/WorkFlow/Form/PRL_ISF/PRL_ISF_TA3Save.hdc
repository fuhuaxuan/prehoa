;全部审批完成后更新流程主表
SQL1=insert into WF_^^ModelCode^^_App (EntGid,FlowGid, DtlGid,AppStat, AppGid,AppName, AppAssign, Note,AppDate,apptype) values(__ENTGID__,##FlowGid##, sys_guid(),decode(##AppAssign##,'同意','12','13'), __USRGID__, __USRNAME__ || '[' || __USRCODE__ || ']', ##AppAssign##, ##Note##,sysdate,40)

;审批人同意，PI <= 0 and 免租期 <= 0 个月 and 总金额 <= 0，流程流程
Sql2=update WF_^^ModelCode^^ set Stat = '30', LastUpdDate = sysdate where EntGid = __ENTGID__ AND  FlowGid = ##FlowGid## and ##sparam1## <= 0 and ##sparam2## <= 0 and ##sparam3## <= 0 and ##TotalFee## <= 0 and ##AppAssign## = '同意'

;审批人否决
Sql4=update WF_^^ModelCode^^ set Stat = '13', LastUpdDate = sysdate where EntGid = __ENTGID__ AND  FlowGid = ##FlowGid## and ##AppAssign## = '否决'

;审批人同意，PI <= 0 and 免租期 <= 0 个月 and 总金额 <= 0 ，更改铺号表信息
sql5=update prl_GROUNDH set fstat = '被租' where fgid in (select fgid from wf_^^ModelCode^^_ground where EntGid=__ENTGID__ and FlowGid = ##FlowGid##) and ##sparam1## <= 0 and ##sparam2## <= 0 and ##sparam3## <= 0 and ##TotalFee## <= 0 and ##AppAssign## = '同意'

