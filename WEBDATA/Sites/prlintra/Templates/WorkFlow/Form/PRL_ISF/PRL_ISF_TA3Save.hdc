;得到模型名称
SqlForCondition1=select Code ModelCode, Name ModelName from wf_Model where ModelGid = ##ModelGid##

;得到业务单据信息
SqlForCondition2= select '['||atype||']'||lessee flownote,nvl(sparam1,0) sparam1,nvl(sparam2,0) sparam2,nvl(sparam3,0) sparam3,nvl(round(TotalFee + OtherTotalFee,2),0) TotalFee,fillusrdeptgid,fillusrgid from WF_^^ModelCode^^ where FlowGid = ##FlowGid##

; --鹏瑞利管辖部门的扩展性：10:部门经理；20:行政经理；30：财务经理；31：出纳；40：商场总经理；50：区域总监；71:资产总监；
SqlForCondition3=select b.usrgid usrA1 from hr_deptpost a,hr_emp b where a.EntGid = __ENTGID__ and a.deptGid = ##fillusrdeptgid## and b.gid = a.empgidex and atype=30 and b.usrgid is not null

;得到业务单据信息
Sql1=select * from WF_^^ModelCode^^ where EntGid = __ENTGID__ and  FlowGid = ##FlowGid##

;取出TB步骤的审批人的邮件
Sql2=select b.Email thisemail,a.gid mailusrgid from Usr a, hr_emp b ,(select ExecGidEx from WF_Task_Define_Exec where TaskDefGid = (select TaskDefGid from WF_Task_Define where LOWER(Code) = lower('^^ModelCode^^_tb') and EntGid = __ENTGID__) ) c where a.gid = b.usrgid and c.ExecGidEx = a.gid

;资产经理 Asset Manager
SqlForCondition4=select b.usrgid usrcc1,b.Email thisemail2 from hr_deptpost a,hr_emp b where a.EntGid = __ENTGID__ and a.deptGid = ##fillusrdeptgid## and b.gid = a.empgidex and atype=71 and b.usrgid is not null

;取出总部的代码
SqlForCondition5=select code2 ZongDept from dept a where code1 = (select substr(code1,0,8)  from dept where gid = ##fillusrdeptgid## and entgid = __ENTGID__) and entgid = __ENTGID__

