SqlForCondition2=select wf.*,decode(mallstat,0,'已发送至Mall，处理失败',10,'已发送至Mall,未接收',100,'已发送至Mall，成功') mallstattext,wf.mallModDate mallModDateText from WF_PRL_ISF wf where FlowGid = ##FlowGid##

LoopSQL2=select a.*,decode(a.apptype,'10','招商经理/LM','20','行政经理','30','财务经理/FM','40','商场总经理/CM','50','区域总监','60','常务理事/COO','70','资产总监/Hd Invest ','80','资产总监/Hd Invest ','90','首席财务官/CFO','100','首席执行官/CEO') apptypetext from WF_PRL_ISF_App a where FlowGid = ##FlowGid##  order by AppDate
