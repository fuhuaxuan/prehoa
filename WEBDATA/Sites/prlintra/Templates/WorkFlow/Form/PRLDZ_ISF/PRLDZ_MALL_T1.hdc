[Permission]
DEFAULT1=FlowGid
DEFAULTValue1=

;判断传入FlowGid 是否存在
SQL1=select 1 from dual where ##FlowGid## is null union select 1 from WF_Flow wf where wf.EntGid = __ENTGID__ and wf.FlowGid = ##FlowGid##
Remind1=对不起，您指定的流程申请单没有找到<script>alert('对不起，您指定的流程申请单没有找到');window.close()</script>

SQL2=select 1 from hr_emp hr,dept d where hr.EntGid = __ENTGID__ and hr.EntGid = d.EntGid and hr.DeptGid = d.Gid and hr.UsrGid = __USRGID__ 
Remind2=您好，您所在的用户不是有效的员工，不能发起申请单！<script language="JavaScript">alert('您好，您所在的用户不是有效的员工，不能发起申请单！');window.close();</script>

[Detail]
;显示，查询得到的结果可以用于HDT显示，从SQL1～SQL20共支持20条
;取出本流程模型信息
SqlForCondition1=select lower(wm.Code) ModelCode, wm.Name ModelName,d.Name StepName from wf_Model wm,WF_Task_Define d where wm.EntGid = d.EntGid(+) and wm.EntGid = __ENTGID__ and wm.ModelGid = d.ModelGid(+) and wm.ModelGid = ##ModelGid## and lower(d.code(+)) = replace(lower(##Url##),'save')

;当FlowGid为空，T1时，获取表头信息
SQL1=select substr(##NewNum##,3) Num,##CurUsrName## FillUsrName,to_char(sysdate,'YYYY.MM.DD HH24:MI') CreateDate from dual where ##FlowGid## is null

;当FlowGid为空，T1时，获取发起人部门
SQL2=select a.Gid FillDeptGid, a.Code2 FillDeptCode, a.Name FillDeptName from Dept a, HR_Emp b where a.EntGid=__ENTGID__ and b.EntGid=__ENTGID__ and b.UsrGid = __USRGID__ and a.Gid = b.DeptGid and ##FlowGid## is null

;判断是T1是发起，还是保存
Sql3=select count(*) IsT1 from WF_PRL_ISF where EntGid = __ENTGID__ and  FlowGid = ##FlowGid##

;TMod时，获取本流程主表信息
Sql4=select f.* from WF_PRL_ISF f where f.EntGid = __ENTGID__ and f.FlowGid = ##FlowGid##

;TMod时，获取本流程已审批的审批人的个数，用于判断是否显示审批人
SQL5=select count(*) IsShow from WF_PRL_ISF_App where EntGid = __ENTGID__ and FlowGid = ##FlowGid## and AppDate is not null

;得到当前用户的部门名称
SQL6=select p.code2 ParentDeptCode from HR_Emp hr,Dept d,dept p where hr.EntGid=__ENTGID__ and hr.EntGid=d.EntGid and d.EntGid = p.EntGid and hr.UsrGid = __USRGID__ and hr.DeptGid = d.Gid and substr(d.Code1,0,8) = p.code1

;TMod时，获取本流程附件信息
LoopSQL1=select Attach_Title ItemGid,Attach_Title ItemName from wf_PRL_ISF_Attach where EntGid = __ENTGID__ and  FlowGid = ##FlowGid## order by attach_title
LoopHDT1=Templates\WorkFlow\Form\PRLDZ_ISF\LoopSql_Option.hdt
LoopVAR1=AttachList

;TMod时，获取本流程审批人信息
LoopSQL2=select a.*,decode(a.apptype,0,'发起人',10,'部门经理',32,'部门总经理',30,'财务经理',35,'总经理',40,'项目总经理',45,'资产经理',50,'资产经理',60,'租赁部总监',80,'财务总监',85,'资产经理',90,'区域总经理',95,'Deputy CEO',100,'CEO') apptypetext  from WF_PRL_ISF_App a where FlowGid = ##FlowGid## and AppDate is not null order by AppDate
LoopHDT2=Templates\WorkFlow\Form\PRLDZ_ISF\Loopsql_App.hdt
LoopVAR2=AppList

;TMod时，获取本流程明细信息
LoopSQL3=select * from wf_PRL_ISF_Dtl where EntGid = __ENTGID__ and FlowGid = ##FlowGid## order by TbID,YearType,FreeRentDate1
LoopHDT3=Templates\WorkFlow\Form\PRLDZ_ISF\Loopsql_DtlMod.hdt
LoopVAR3=DtlList

;显示oldFlowGid单位信息
LoopSQL4=select * from wf_PRL_ISF_ground where EntGid = __ENTGID__ and FlowGid = ##FlowGid## order by fNo
LoopHDT4=Templates\WorkFlow\Form\PRLDZ_ISF\LoopSql_UnitList.hdt
LoopVAR4=UnitList

;取出业态
LoopSQL5=select code ItemGid,'['||code||']'||name ItemName from Tbusrange where length(code) = 6 order by code
LoopHDT5=Templates\WorkFlow\Form\PRLDZ_ISF\LoopSql_Option.hdt
LoopVAR5=Tradelist

