[Permission]
;判断FlowGid 是否为空
LIMIT1=FlowGid
LIMITDesc1=StringSize[,0]
LIMITRemind1=对不起，您未指定流程信息！请从正确的路径进入

;判断传入FlowGid 是否存在
SQL1=select 1 from WF_Flow where EntGid = __ENTGID__ and FlowGid = ##FlowGid##
Remind1=对不起，您指定的流程申请单没有找到<script>alert('对不起，您指定的流程申请单没有找到');window.close()</script>

[Detail]
;显示，单条数据的查询，可以用于HDT显示，有select命令。从SQL1～SQL20共支持20条
;取出本流程模型信息
SqlForCondition1=select wm.Code ModelCode, wm.Name ModelName from wf_Model wm where wm.EntGid = __ENTGID__ and wm.ModelGid = ##ModelGid##

;取出发起人的部门代码
SqlForCondition2=select substr(f.FillDeptCode,0,4) ProCode from wf_PrlDZ_Area f where f.EntGid = __ENTGID__ and f.FlowGid = ##FlowGid##

;显示本流程主表信息
SQL1=select wf.* from wf_PrlDZ_Area wf where EntGid = __ENTGID__ and  FlowGid = ##FlowGid##

;显示本流程明细信息
LoopSQL3=select * from wf_PrlDZ_Area_Dtl where EntGid = __ENTGID__ and FlowGid = ##FlowGid## order by Atype,Line
LoopHDT3=Templates\WorkFlow\Form\^^ModelCode^^\Loopsql_DtlMod.hdt
LoopVAR3=DtlList

;显示本流程已审批审批人信息
LoopSQL2=select a.AppCode,a.AppName,a.AppAssign,a.AppDate,a.AppMemo,a.AppOrder,decode(a.apptype,0,'发起人',10,'部门经理',20,'部门总监',35,'总经理/项目总监',40,'项目总经理',50,'资产经理',60,'区域部门总监',80,'区域总经理',92,'首席投资官',95,'中国区CEO',99,'发起人确认') apptypetext from wf_PrlDZ_Area_App a where FlowGid = ##FlowGid## union select ExecCode AppCode,ExecName AppName,decode(t.stat,3,'已阅读','') AppAssign,ETIME AppDate,'' AppMemo,100 AppOrder,t.Name apptypetext from v_wf_task t where FlowGid = ##FlowGid## and t.ISMCF = 1 order by AppDate,AppOrder
LoopHDT2=Templates\WorkFlow\Form\^^ModelCode^^\Loopsql_AppPrint.hdt
LoopVAR2=AppList

[AddtionalSQL]
;操作，没有返回的结果，是在数据库中执行，有Insert, Update, Delete命令。从SQL1～SQL20共支持20条
SQL1=update Wf_Flow set PrintTime = PrintTime + 1 where EntGid = __ENTGID__ and FlowGid = ##FlowGid##

