[Permission]
;判断FlowGid 是否为空
LIMIT1=FlowGid
LIMITDesc1=StringSize[,0]
LIMITRemind1=对不起，您未指定流程信息！请从正确的路径进入

;判断传入FlowGid 是否存在
SQL1=select 1 from WF_Flow where EntGid = __ENTGID__ and FlowGid = ##FlowGid##
Remind1=对不起，您指定的流程申请单没有找到<script>alert('对不起，您指定的流程申请单没有找到');window.close()</script>

;检查判断的Sql语句，查不到结果是报错，从SQL1～SQL5，共可以写5句
SQL2=select 1 from dual where ##AppAssign## <> '同意' union select 1 from v_Post v where v.EntGid = __ENTGID__ and v.deptGid = ##FillUsrDeptGid## and v.atype=71 and ##AppAssign## = '同意'
Remind2=对不起，下一步审批人资产经理未找到，请与管理员联系。<script>alert('对不起，下一步审批人资产经理未找到，请与管理员联系。');history.back()</script>

[Detail]
;显示，单条数据的查询，可以用于HDT显示，有select命令。从SQL1～SQL20共支持20条
;取出本流程模型信息
SqlForCondition1=select wm.code ModelCode,replace(upper(wm.code),'DZ','') ModelCodeT, wm.Name ModelName,d.Name StepName from wf_Model wm,WF_Task_Define d where wm.EntGid = d.EntGid(+) and wm.EntGid = __ENTGID__ and wm.ModelGid = d.ModelGid(+) and wm.ModelGid = ##ModelGid## and lower(d.code(+)) = replace(lower(##Url##),'save')

;审批备注
SQLForCondition2=select ##CurUsrName##||':'||##AppAssign##||' - ' || decode(FeeNum,'','','费用单：' || FeeNum||'-' || PartNum) || '；付款金额：' || PayFee || '；付款备注：' || Memo AppFlowMemo from wf_^^ModelCodeT^^ where EntGid = __ENTGID__ AND  FlowGid = ##FlowGid##
;判断是否取下一格审批
SqlForCondition3=select count(*) IsNext from wf_^^ModelCodeT^^ where EntGid = __ENTGID__ AND  FlowGid = ##FlowGid## and nvl(payfee,0) > nvl(bVALUE,0) and (nvl(bVALUE,0) +nvl(cVALUE,0) +nvl(dVALUE,0)+nvl(eVALUE,0)) > 0

;取出下一步审批人
SQLForCondition4=select v.PostGid TB0Gid from v_Post v where v.EntGid = __ENTGID__ and v.deptGid = ##FillUsrDeptGid## and v.atype=20

SQLForCondition5=select v.PostGid TB1Gid from v_Post v where v.EntGid = __ENTGID__ and v.deptGid = ##FillUsrDeptGid## and v.atype=71

;判断是否需求B0 区域财务经理审批，acgcode ('1.01','1.02','1.03','2.01','2.02','2.03')
SqlForCondition7=select count(*) IsB0 from wf_^^ModelCodeT^^ f where f.EntGid = __ENTGID__ AND f.FlowGid = ##FlowGid## and exists (select 1 from v_acg a where a.entgid = f.entgid and a.gid = f.AcgTwoGid and a.code in ('1.01','1.02','1.03','2.01','2.02','2.03'))

[AddtionalSQL]
;操作，没有返回的结果，是在数据库中执行，有Insert, Update, Delete命令。从SQL1～SQL20共支持20条
;更新本流程中主表信息
Sql1=update wf_^^ModelCodeT^^ set (LastUpdDate,Stat) = (select sysdate,##AppAssign## from dual) where EntGid = __ENTGID__ AND  FlowGid = ##FlowGid## 

;插入本流程中审批人二信息
SQL2=insert into wf_^^ModelCodeT^^_App (EntGid,FlowGid,dtlgid,AppGid,AppCode,AppName,AppType,AppAssign,note,AppDate) select __ENTGID__,##FlowGid##,sys_guid(),__USRGID__,__USRCODE__,__USRNAME__,40,##AppAssign##, ##note##,sysdate from dual

