[Permission]
;判断FlowGid 是否为空
LIMIT1=FlowGid
LIMITDesc1=StringSize[,0]
LIMITRemind1=对不起，您未指定流程信息！请从正确的路径进入

;判断传入FlowGid 是否存在
SQL1=select 1 from WF_Flow where EntGid = __ENTGID__ and FlowGid = ##FlowGid##
Remind1=对不起，您指定的流程申请单没有找到<script>alert('对不起，您指定的流程申请单没有找到');window.close()</script>

;检查判断的Sql语句，查不到结果是报错，从SQL1～SQL5，共可以写5句
SQL2=select 1 from dual where ##AppAssign## <> '同意' union select 1 from v_Post v where v.EntGid = __ENTGID__ and v.deptGid = ##FillDeptGid## and v.atype=80 and ##AppAssign## = '同意'
Remind2=对不起，下一步审批人资产总监未找到，请与管理员联系。<script>alert('对不起，下一步审批人资产总监未找到，请与管理员联系。');history.back()</script>

[Detail]
;显示，查询得到的结果可以用于HDT显示，从SQL1～SQL20共支持20条
;取出本流程模型信息
SqlForCondition1=select wm.Code ModelCode, wm.Name ModelName,d.Name StepName from wf_Model wm,WF_Task_Define d where wm.EntGid = d.EntGid(+) and wm.EntGid = __ENTGID__ and wm.ModelGid = d.ModelGid(+) and wm.ModelGid = ##ModelGid## and lower(d.code(+)) = replace(lower(##Url##),'save')

;审批备注
SQLForCondition2=select ##CurUsrName##||':'||##AppAssign##||' - 报销人：' || ApplyUsrName ||'；公司：' || ComName ||'；金额：'|| SumFee ||'元' AppFlowMemo,nvl(f.SumFee,0) SumFee from WF_^^ModelCode^^ f where EntGid = __ENTGID__ AND  FlowGid = ##FlowGid## 

;鹏瑞利管辖部门的扩展性：
;5:部门助理;
;10:部门经理(招商经理);
;20:区域财务经理;
;25:财务会计;
;30:财务经理;
;31:出纳付款;
;35:执行总经理;
;40:项目总经理;
;50:区域总监;
;71:资产经理 AssetManager
;取出下一步审批人
SQLForCondition5=select v.PostGid TC1Gid from v_Post v where v.EntGid = __ENTGID__ and v.deptGid = ##FillDeptGid## and v.atype=80

;取出流程信息，用于邮件
Sql1=select f.* from WF_^^ModelCode^^ f where f.EntGid = __ENTGID__ and f.FlowGid = ##FlowGid##

;取出邮件
;同意并下一步：取出下一步执行人Email
LoopSQL1=select * from wf_^^ModelCode^^_Dtl where EntGid = __ENTGID__ and FlowGid = ##FlowGid## order by AcgCode
LoopHDT1=Templates\WorkFlow\Form\^^ModelCode^^\LoopSql_EmailDtl.hdt
LoopVAR1=EmailDtl

;取出邮件
;同意并下一步：取出下一步执行人Email
LoopSQL2=select distinct v.Email from v_Post v where v.EntGid = __ENTGID__ and v.deptGid = ##FillDeptGid## and v.atype in (80) and ##AppAssign## = '同意' and ##SumFee## > 10000 union select hr.Email from hr_emp hr where hr.EntGid= __ENTGID__ and hr.usrgid = ##FillUsrGid## and ##AppAssign## = '同意' and ##SumFee## <= 10000
LoopHDT2=Templates\WorkFlow\Form\^^ModelCode^^\LoopSql_Email.hdt
LoopVAR2=EmailList

[AddtionalSQL]
;操作，没有返回的结果，是在数据库中执行，由Insert, Update, Delete命令。从SQL1～SQL20共支持20条
;更新本流程中主表信息
Sql1=update WF_^^ModelCode^^ set (LastUpdDate,Stat) = (select sysdate,##AppAssign## from dual) where EntGid = __ENTGID__ AND  FlowGid = ##FlowGid## 

;插入本流程中审批人二信息
SQL2=insert into Wf_^^ModelCode^^_App (EntGid,ModelGid,FlowGid,Gid,AppGid,AppCode,AppName,AppType,AppAssign,AppMemo,AppDate) select __ENTGID__,##ModelGid##,##FlowGid##,sys_guid(),__USRGID__,__USRCODE__,__USRNAME__,50,##AppAssign##, ##AppMemo##,sysdate from dual

