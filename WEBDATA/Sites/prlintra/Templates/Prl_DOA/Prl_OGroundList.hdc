[Permission]
;赋予默认值，共可赋予10个默认值，都是字符串形式
;DefaultX为变量名
;DefaultValueX为设置的默认值
DEFAULT1=sel_Type
DEFAULTValue1=1

[Detail]
;显示，单条数据的查询，可以用于HDT显示，有select命令。从SQL1～SQL20共支持20条
;取出当前页面的信息
SqlForCondition1=select Name ModeleName,Atype ModeleAtype,to_char(sysdate,'YYYY') CurYear from rt where lower(id) = lower(##Url##)

;判断当前用户是否是管理员
SqlForCondition2=

;判断当前用户的项目
SqlForCondition3=

[AddtionalSQL]
;操作，没有返回的结果，是在数据库中执行，有Insert, Update, Delete命令。从SQL1～SQL20共支持20条
SQL1=

[AddtionalSQLUseWhere]
;显示，单条数据的查询，可以用于HDT显示，有select命令。从SQL1～SQL20共支持20条，同时会使用上面的[where]中的sql语句
;示例
Sql1=

[Master]
;显示，多条记录的查询，可在HDT页面中直接使用<HDLoop>显示，并能够提供翻页。and '^^NotFirst^^' = '1'
SQL=select a.floorno, a.farea , nvl(b.tarea, 0) yArea, a.farea - nvl(b.tarea, 0) nArea from (select g.floorno, nvl(sum(g.farea), 0) farea from v_PRL_Ground_Floor g where g.fno like '^^sel_Type^^%' group by g.floorno) a, (select g.floorno,  nvl(sum(g.farea), 0) farea,  nvl(sum(g.tarea), 0) tarea from v_Prl_OIsf_Floor g where g.fno like '^^sel_Type^^%' group by g.floorno) b where a.floorno = b.floorno(+) 

Count=select count(*) from (select g.floorno, nvl(sum(g.farea), 0) farea from v_PRL_Ground_Floor g where g.fno like '^^sel_Type^^%' group by g.floorno) a, (select g.floorno,  nvl(sum(g.farea), 0) farea,  nvl(sum(g.tarea), 0) tarea from v_Prl_OIsf_Floor g where g.fno like '^^sel_Type^^%' group by g.floorno) b where a.floorno = b.floorno(+) 


[DefaultWhere]
;多条记录的查询的默认条件
DefaultWhere=

[Where]
;多条记录的查询的条件，可以根据页面传入的变量组合成
1=
[GroupBy]
;多条记录的查询的分组
GroupBy=

[DefaultOrderBy]
;多条记录的查询的默认排序
DefaultOrderBy=to_number(replace(replace(a.floorno,'F',''),'B','-'))

[OrderBy]
;多条记录的查询的排序,支持多重排序排序的先后顺序由序号决定
1=##selOrderBy##

[Title]
;页面的标题，从Text1～Text30共支持30条

Text1=^^CurYear^^-^^sel_Type^^号楼-^^ModeleName^^

;是否翻页：1是，0否
ChangePage=0

;翻页时的每页记录数，如果没有的话，则取HDNet默认值20
ChangePageRow=100

;写入数据库日志
